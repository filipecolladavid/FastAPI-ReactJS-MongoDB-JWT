REFRESH_TOKEN_EXPIRES_IN=60
JWT_ALGORITHM=RS256

CLIENT_ORIGIN=http://localhost:3000

JWT_PRIVATE_KEY= GENERATE RESPECTIVE PRIVATE KEY
JWT_PUBLIC_KEY= GENERATE PUBLIC KEY
